{
  "name": "flattracker-mobile",
  "version": "0.0.15",
  "scripts": {
    "postinstall": "bower install && gulp build",
    "start": "gulp watch"
  },
  "dependencies": {
    "bower": "*",
    "coffee-script": "*",
    "cordova": "^5.4.1",
    "gulp": "*",
    "gulp-add-src": "*",
    "gulp-angular-templatecache": "*",
    "gulp-coffee": "*",
    "gulp-concat": "*",
    "gulp-expect-file": "*",
    "gulp-filter": "*",
    "gulp-jade": "*",
    "gulp-ng-constant": "*",
    "gulp-plug": "*",
    "gulp-plumber": "*",
    "gulp-rename": "*",
    "gulp-replace": "*",
    "gulp-rimraf": "*",
    "gulp-sass": "^2.0.1",
    "gulp-sourcemaps": "*",
    "gulp-stylus": "*",
    "gulp-webserver": "*",
    "main-bower-files": "^2.5.0",
    "require-dir": "^0.1.0",
    "wiredep": "*"
  },
  "cordovaPlugins": [
    "cordova-plugin-splashscreen",
    {
